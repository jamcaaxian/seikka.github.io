                function Get_List() {
                    $.ajax({
                        type: "post",
                        url: "/api/EarnUser/EarnRecommend",
                        contentType: 'application/json',
                        data: JSON.stringify({
                            "uid": uid
                        }),
                        success: function (data) {
                            if (data == "" || data == null) {
                                if (!$('.result-tip').hasClass('result-tip')) {
